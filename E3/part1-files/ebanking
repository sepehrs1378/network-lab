<script type="text/javascript" language="JavaScript">
    function changeLocale() {
        var intCustId = dwr.util.getValue("intCustId");
        var intEbId = dwr.util.getValue("intEbId");

        dwr.engine.beginBatch();
        DWRServices.changeLocale('', function(data) {
            handleServerLocaleResult(data);
        });

        dwr.engine.endBatch({errorHandler:function(errorString, exception) {
            if(errorString == 'The specified call count is not a number: null'){
                alert('لطفا دوباره وارد شويد.');
            }else{
                alert('مشتري محترم , در حال حاضر سرويس مورد نظر فعال نمي باشد . (1)');
            }
        }
        });
    }

    dwr.engine.setErrorHandler(errorHandlerFn);//global exception
    function errorHandlerFn(message, exception) {
        alert('مشتري محترم , در حال حاضر سرويس مورد نظر فعال نمي باشد . (1)');
    }
    dwr.engine.setTextHtmlHandler(//session timeout
            function() {
                alert('لطفا دوباره وارد شويد.');}
            );
    function handleServerLocaleResult(data) {
            getElement('localeButton').value = data ;
    }

    function updateFormsElements(csrfIdValue){
        var forms = document.forms;
        for(i= 0 ;i<forms.length ; i++){
            if(forms[i].clientCsrfId != "")
                forms[i].clientCsrfId.value  = csrfIdValue;
        }
    }
</script>
