<script type="text/javascript" language="javascript">
    var otpSmsCounterStarted = false;
    var otpSmsSecondsTimer = 59;
    var otpSmsTimerHandler = null;
    function sendOtpSms_click()
    {
        if(validateLoginForm()){
            if (!otpSmsCounterStarted)
                sendOtpSms_Handler();
            else
                alert("کاربر گرامی ، با سپری شدن زمان اعلام شده امکان ارسال مجدد پیامک یکبار رمز وجود دارد. لطفا منتظر بمانید");
        }
    }

    function sendOtpSms_Handler()
    {
        var otpSmsElm = document.all['otpSmsBtn'];
        var counterTd = otpSmsElm.children[0].rows[0].cells[0];
        if (otpSmsCounterStarted)
        {
            if (otpSmsSecondsTimer == 0 || otpSmsSecondsTimer < 0)
            {
                counterTd.innerHTML = '--';
                otpSmsSecondsTimer = 59;
                otpSmsCounterStarted = false;
                clearTimeout(otpSmsTimerHandler);
                return;
            }
            else
            {
                otpSmsSecondsTimer -= 1;
            }
            if (otpSmsSecondsTimer >= 10)
                counterTd.innerHTML = otpSmsSecondsTimer;
            else
                counterTd.innerHTML = '0' + otpSmsSecondsTimer;
        }
        otpSmsTimerHandler = setTimeout('sendOtpSms_Handler()', 1000);
        if (otpSmsCounterStarted)
            return;

        otpSmsSecondsTimer = 59;
        otpSmsCounterStarted = true;
        counterTd.innerHTML = otpSmsSecondsTimer;

        dwr.engine.beginBatch();
        var csrfId = document.changeTabX.clientCsrfId.value;
        var userId = document.loginForm.userId.value;
        var password = document.loginForm.password.value;
        var captchaValEnc = document.all['textPattern_enc'].value;
        var captchaValue =  document.loginForm.textPattern.value;
        DWRServices.sendSmsOtpCodeForLogin(csrfId, userId, password, captchaValue, captchaValEnc, function(dwrResult) {
//           updateFormsElements(dwrResult.callbackcsrfId);
            if(dwrResult.success) {

                reloadCaptcha();
                document.loginForm.textPattern.value = '';

                if(dwrResult.data==60){
                    alert("پیامک یکبار رمز ارسال گردید");
                }else {
                    otpSmsSecondsTimer = dwrResult.data;
                    alert("کاربر گرامی ، با سپری شدن زمان اعلام شده امکان ارسال مجدد پیامک یکبار رمز وجود دارد. لطفا منتظر بمانید");
                }

            }
            else {
                reloadCaptcha();
                document.loginForm.textPattern.value = '';
                otpSmsSecondsTimer = 0;
                alert(dwrResult.errorText);
            }
        });

        dwr.engine.endBatch({errorHandler:function(errorString, exception) {
            if (errorString == 'The specified call count is not a number: null') {
                reloadCaptcha();
                document.loginForm.textPattern.value = '';
                otpSmsSecondsTimer = 0;
                alert("لطفا دوباره وارد شويد.");
            } else {
                reloadCaptcha();
                document.loginForm.textPattern.value = '';
                otpSmsSecondsTimer = 0;
                alert("مشتري محترم , در حال حاضر سرويس مورد نظر فعال نمي باشد . (1)");
            }
            return false;
        }
        });

        dwr.engine.setErrorHandler(errorHandlerFn);
        function errorHandlerFn(message, exception) {
            reloadCaptcha();
            document.loginForm.textPattern.value = '';
            otpSmsSecondsTimer = 0;
            alert("مشتري محترم , در حال حاضر سرويس مورد نظر فعال نمي باشد . (1)");
            return false;
        }

        dwr.engine.setTextHtmlHandler(
                function() {
                    reloadCaptcha();
                    document.loginForm.textPattern.value = '';
                    otpSmsSecondsTimer = 0;
                    alert("لطفا دوباره وارد شويد.");
                    return false;
                }
        );

    }

</script>


